	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>Species 13. (Mus. Cuming.)</Unicode></TextEquiv></TextRegion>
	<TextRegion id="r64" type="paragraph">
	<Coords>
	<Point x="1211" y="399"/>
	<Point x="2102" y="399"/>
