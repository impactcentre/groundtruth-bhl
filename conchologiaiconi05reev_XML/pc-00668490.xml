	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>Hab. Senegal.</Unicode></TextEquiv></TextRegion>
	<TextRegion id="r20" type="paragraph">
	<Coords>
	<Point x="1286" y="1720"/>
