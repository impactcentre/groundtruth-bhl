	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>Hab.—?</Unicode></TextEquiv></TextRegion>
	<TextRegion id="r32" type="paragraph">
	<Coords>
	<Point x="1391" y="2258"/>
