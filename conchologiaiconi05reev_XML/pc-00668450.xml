	<Point x="1344" y="2342"/></Coords>
	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>Hab.—?</Unicode></TextEquiv></TextRegion>
	<TextRegion id="r29" type="paragraph">
	<Coords>
	<Point x="1344" y="2358"/>
