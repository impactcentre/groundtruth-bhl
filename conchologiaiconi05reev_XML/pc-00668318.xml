	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>Species 85. (Mus. Cuming.)</Unicode></TextEquiv></TextRegion>
	<TextRegion id="r24" type="paragraph">
	<Coords>
	<Point x="1313" y="1673"/>
	<Point x="2209" y="1673"/>
