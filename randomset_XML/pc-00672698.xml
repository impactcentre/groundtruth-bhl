	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>COOKING WILD DUCKS.</Unicode></TextEquiv></TextRegion>
	<TextRegion id="r2" type="page-number">
	<Coords>
	<Point x="1586" y="225"/>
	<Point x="1707" y="225"/>
	<Point x="1707" y="284"/>
	<Point x="1586" y="284"/></Coords>
	<TextEquiv>
