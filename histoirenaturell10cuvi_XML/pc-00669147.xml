	<Coords>
	<Point x="438" y="1592"/>
	<Point x="1318" y="1592"/>
	<Point x="1318" y="1668"/>
	<Point x="438" y="1668"/></Coords>
	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>D. 8/25; A. 3/24; C. 16; P. 15; V. 1/5,</Unicode></TextEquiv></TextRegion>
	<TextRegion id="r7" type="paragraph">
	<Coords>
	<Point x="169" y="1680"/>
	<Point x="1194" y="1680"/>
	<Point x="1194" y="1763"/>
	<Point x="169" y="1763"/></Coords>
	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>et une longueur de cinq pouces.</Unicode></TextEquiv></TextRegion>
	<TextRegion id="r8" type="paragraph">
	<Coords>
	<Point x="161" y="1766"/>
	<Point x="1586" y="1766"/>
	<Point x="1586" y="1940"/>
	<Point x="161" y="1940"/></Coords>
	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>Les Arabes, dit-il, l’appellent à Djedda tægà,
mot qui signiﬁe fauve.</Unicode></TextEquiv></TextRegion>
