	<TextEquiv>
	<PlainText></PlainText>
	<Unicode>Des Acanthures.</Unicode></TextEquiv></TextRegion>
	<TextRegion id="r4" type="paragraph">
	<Coords>
	<Point x="360" y="686"/>
